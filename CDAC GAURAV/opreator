#include <iostream>
using namespace std;

class Box
{
private:
    double length;
    double breadth;
    double height;

public:
    // Input function
    void input()
    {
        cout << "Enter length, breadth, height: ";
        cin >> length >> breadth >> height;
    }

    // Function to calculate volume
    double volume() const
    {
        return length * breadth * height;
    }

    // Operator overloading >
    bool operator>(Box &b)
    {
        return this->volume() > b.volume();
    }
};

int main()
{
    Box b1, b2;

    // Object pointers
    Box *p1 = &b1;
    Box *p2 = &b2;

    cout << "--- Enter details of Box 1 ---\n";
    p1->input();

    cout << "\n--- Enter details of Box 2 ---\n";
    p2->input();

    cout << "\nVolume of Box 1: " << p1->volume() << endl;
    cout << "Volume of Box 2: " << p2->volume() << endl;

    if (*p1 > *p2)
        cout << "\nBox 1 has greater volume.\n";
    else
        cout << "\nBox 2 has greater volume.\n";

    return 0;
}
