#include <iostream>
using namespace std;

class Employee
{
    int salary;

public:
    void input()
    {
        cin >> salary;
    }

    bool operator>(Employee &e)
    {
        return salary > e.salary;
    }
};

int main()
{
    Employee e1, e2;
    Employee *p1 = &e1;
    Employee *p2 = &e2;

    cout << "Enter salary of employee 1: ";
    p1->input();
    cout << "Enter salary of employee 2: ";
    p2->input();

    if (*p1 > *p2)
        cout << "Employee 1 has higher salary";
    else
        cout << "Employee 2 has higher salary";

    return 0;
}
